{
  "manifest_version": 3,
  "name": "Tumblr Tag",
  "short_name": "tumbtag",
  "description": "Post your list of favorite tags on Tumblr in one click.",
  "version": "3.5.7",
  "icons": {
    "16": "icons/16x16.png",
    "48": "icons/48x48.png",
    "128": "icons/128x128.png"
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://www.tumblr.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.tumblr.com/*"
      ],
      "all_frames": true,
      "js": [
        "jquery-3.1.1.min.js",
        "content.js"
      ]
    }
  ]
}
